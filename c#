using System;

public class Class1
{
	public Class1()
	{
        template<typename T>
        T GCD_subtraction(T a, T b)
{
    while (a != b)
        if (a > b)
            a = a - b;
        else
            b = b - a;
    return a;
}

    template<typename T>
    T GCD_modulo(T a, T b)
{
    T c;
    while (b != 0){
        c = a % b;
        a = b;
        b = c;
    }
    return a;
}

	}
}